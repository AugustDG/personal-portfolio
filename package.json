{
  "name": "personal-portfolio",
  "version": "1.0.0",
  "description": "My personal portfolio project!",
  "main": "index.js",
  "scripts": {
    "build-dev": "postcss src/styles.css -o dev/css/final.css --env development -w true",
    "build-prod": "copyfiles -f dev/*.html public/ && copyfiles -f dev/js/*.js public/js/ && copyfiles -f dev/img/*.webp public/img/ && postcss src/styles.css -o public/css/final.css --env production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "autoprefixer": "^10.2.3",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-cli": "^0.1.2",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "postcss": "^8.2.4"
  }
}
